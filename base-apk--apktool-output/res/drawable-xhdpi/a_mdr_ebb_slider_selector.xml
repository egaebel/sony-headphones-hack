<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <layer-list>
            <item android:id="@android:id/background">
                <nine-patch android:src="@drawable/a_mdr_ebb_slider_background" android:dither="true" />
            </item>
            <item android:id="@android:id/progress">
                <clip>
                    <nine-patch android:src="@drawable/a_mdr_ebb_slider_foreground" android:dither="true" />
                </clip>
            </item>
        </layer-list>
    </item>
</selector>